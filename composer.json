{
    "name": "selimsalihovic/pikpay-php",
    "type": "library",
    "description": "Payment Processing Library for PikPay",
    "keywords": [
        "selimsalihovic",
        "pikpay-php"
    ],
    "homepage": "https://github.com/selimsalihovic/pikpay-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Selim Salihovic",
            "email": "selim.salihovic@gmail.com",
            "homepage": "http://selimsalihovic.github.io/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "SelimSalihovic\\PikPay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SelimSalihovic\\PikPay\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
